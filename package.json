{
  "name": "maxmind-geolite-mirror",
  "version": "1.0.5",
  "homepage": "https://github.com/msimerson/maxmind-geolite-mirror",
  "description": "Mirror maxmind dbs from geolite.maxmind.com",
  "author": {
    "name": "Matt Simerson",
    "url": "http://matt.simerson.net"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/msimerson/maxmind-geolite-mirror.git"
  },
  "bugs": {
    "url": "https://github.com/msimerson/maxmind-geolite-mirror/issues"
  },
  "bin": {
    "maxmind-geolite-mirror": "./bin/maxmind-geolite-mirror"
  },
  "config": {
    "blanket": {
      "pattern": "lib"
    }
  },
  "main": "bin/maxmind-geolite-mirror",
  "keywords": [
    "geolite",
    "geoip",
    "download",
    "mirror",
    "maxmind"
  ],
  "scripts": {
    "test": "./node_modules/.bin/mocha",
    "coveralls": "COVERAGE=1 ./node_modules/.bin/mocha --require blanket --reporter mocha-lcov-reporter | ./node_modules/coveralls/bin/coveralls.js"
  },
  "devDependencies": {
    "blanket": "^1.1.6",
    "coveralls": "^2.11.2",
    "grunt": "^0.4.5",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-jshint": "^0.11.0",
    "grunt-coveralls": "^1.0.0",
    "grunt-mocha-test": "^0.12.7",
    "jshint": "^2.6.0",
    "mocha": "^2.1.0",
    "mocha-lcov-reporter": "0.0.1",
    "rewire": "^2.3.4"
  },
  "license": "MIT",
  "files": [
    "bin",
    "lib",
    "test",
    "Gruntfile.js"
  ]
}
